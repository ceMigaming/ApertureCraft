{
    "multipart": [
        {   "when": { "OR": [
            {"north": "none", "east": "none", "south": "none", "west": "none"},
            {"north": "side|up", "east": "side|up" },
            {"east": "side|up", "south": "side|up" },
            {"south": "side|up", "west": "side|up"},
            {"west": "side|up", "north": "side|up"}
        ]},
        "apply": { "model": "aperturecraft:block/indicator_light_dot" }
    },
        {   "when": { "OR": [
            {"north": "side|up", "east": "none", "south": "none", "west": "none" },
            {"north": "none", "east": "side|up", "south": "none", "west": "none" },
            {"north": "none", "east": "none", "south": "side|up", "west": "none" },
            {"north": "none", "east": "none", "south": "none", "west": "side|up" },
            {"north": "none", "east": "side|up", "south": "none", "west": "side|up" },
            {"north": "side|up", "east": "none", "south": "side|up", "west": "none" }
        ]},
        "apply": { "model": "aperturecraft:block/indicator_light_dot_alt" }
    },
    {   "when": { "OR": [
            { "north": "side|up" },
            { "north": "none", "east": "none", "south": "side|up", "west": "none" }
        ]},
        "apply": { "model": "aperturecraft:block/indicator_light_side" }
    },
    {   "when": { "OR": [
            { "south": "side|up" },
            { "north": "side|up", "east": "none", "south": "none", "west": "none" }
        ]},
        "apply": { "model": "aperturecraft:block/indicator_light_side", "y": 180 }
    },
    {   "when": { "OR": [
            { "east": "side|up" },
            { "north": "none", "east": "none", "south": "none", "west": "side|up" }
        ]},
        "apply": { "model": "aperturecraft:block/indicator_light_side", "y": 90 }
    },
    {   "when": { "OR": [
            { "west": "side|up" },
            { "north": "none", "east": "side|up", "south": "none", "west": "none" }
        ]},
        "apply": { "model": "aperturecraft:block/indicator_light_side", "y": 270 }
    },
    {   "when": { "north": "up" },
        "apply": { "model": "aperturecraft:block/indicator_light_up" }
    },
    {   "when": { "east": "up" },
        "apply": { "model": "aperturecraft:block/indicator_light_up", "y": 90 }
    },
    {   "when": { "south": "up" },
        "apply": { "model": "aperturecraft:block/indicator_light_up", "y": 180 }
    },
    {   "when": { "west": "up" },
        "apply": { "model": "aperturecraft:block/indicator_light_up", "y": 270 }
    }
]
}